	keywords = {Mathematical model, Force, Torque, electric vehicle, tricycle, electric vehicles, Computational modeling, velocity control, torque control, nonlinear control systems, ergonomics, compensation, electric drive, Generators, Kinematics, Load modeling, dynamics, generator, pedaling model, pedaling-by-wire system, control system, parameters adaptation, chi2 coefficient, computation complexity, continuous analytical model, cyclist pedaling, electric trike, fitting process, Gauss-Newton algorithm, geometric approach, homogeneous analytical model, inverse kinematics, kinematic equations, kinematics, kinematics relations, nonlinear compensation, pedals ergonomic load torque angular distribution, pedals speed ripples reduction, vector approach, vehicle dynamics, vehicle photography},
	pages = {1164--1169},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Bodziu\\Zotero\\storage\\FGPE2P77\\7575303.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\Bodziu\\Zotero\\storage\\NNCEIJVQ\\Fabianski i Wicher - 2016 - Model and analysis of pedals ergonomic load torque.pdf:application/pdf}
